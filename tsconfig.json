    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
        "*": ["src/*"]
    }
  },
  "include": [
    "./src/**/*"
  ],
}